
LIB = libmat.lib

OBJS = matset.obj material.obj matela/elastic.obj \
        mymat/mymat.obj \
       mymatm/mymatm.obj  Stress_Prin.obj \
		 mohr/mohr.obj Prin2Stress.obj mymatfq/mymatfq.obj \
		 mymatmpalpha/mymatmpalpha.obj
 

$(LIB): $(OBJS)
	xilink /Qmkl /debug /Lib /out:$(LIB) $(OBJS)

material.obj:material.cpp
	icl /debug /c material.cpp

matset.obj:matset.cpp material.h
	icl /debug /c matset.cpp

matela/elastic.obj:matela/elastic.cpp matela/elastic.h
	cd matela
	icl /debug /c elastic.cpp
	cd ..

mymat/mymat.obj:mymat/mymat.cpp mymat/mymat.h
	cd mymat
	icl /debug /c mymat.cpp
	cd ..

mymatfq/mymatfq.obj:mymatfq/mymatfq.cpp mymatfq/mymatfq.h
	cd mymatfq
	icl /debug /c mymatfq.cpp
	cd ..

mymatm/mymatm.obj:mymatm/mymatm.cpp mymatm/mymatm.h
	cd mymatm
	icl /debug /c mymatm.cpp
	cd ..

mohr/mohr.obj:mohr/mohr.cpp mohr/mohr.h 
	cd mohr
	icl /debug /c mohr.cpp
	cd ..

Stress_Prin.obj:Stress_Prin.cpp Stress_Prin.h
	icl /debug /c /Qmkl Stress_Prin.cpp

Prin2Stress.obj:Prin2Stress.cpp Prin2Stress.h
	icl /debug /c Prin2Stress.cpp

mymatmpalpha/mymatmpalpha.obj:mymatmpalpha/mymatmpalpha.cpp mymatmpalpha/mymatmpalpha.h
	cd mymatmpalpha
	icl /debug /c mymatmpalpha.cpp
	cd ..
